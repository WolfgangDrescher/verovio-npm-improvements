{
    "name": "verovio",
    "version": "3.9.0",
    "description": "This is the stable version of the verovio package",
    "type": "module",
    "main": "dist/verovio.umd.js",
    "module": "dist/verovio.es.js",
    "exports": {
        ".": {
            "import": "./dist/verovio.es.js",
            "require": "./dist/verovio.umd.cjs"
        }
    },
    "files": [
        "dist",
        "modules"
    ],
    "scripts": {
        "build": "rollup --config"
    },
    "keywords": [
        "music",
        "music notation",
        "music engraving",
        "mei",
        "music encoding",
        "svg"
    ],
    "author": "Laurent Pugin and others",
    "license": "LGPL 3",
    "repository": {
        "type": "git",
        "url": "https://github.com/rism-digital/verovio.git"
    },
    "bugs": {
        "url": "https://github.com/rism-digital/verovio/issues"
    },
    "homepage": "https://www.verovio.org",
    "engines": {
        "node": "^12.22.0 || >= 14.17.0"
    },
    "devDependencies": {
        "rollup": "^2.73.0"
    }
}
